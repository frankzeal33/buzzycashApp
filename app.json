{
  "expo": {
    "name": "buzzycash",
    "slug": "buzzycash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "buzzycash",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#111625"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.franklin93.buzzycash"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/default-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#111625"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f83d3144-0a7f-4052-9c64-79ee67be8157"
      }
    }
  }
}
